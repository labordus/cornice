#: -:308
msgid ""
" SPACE or PG_DOWN:     Show next image\n"
" BACKSPACE or PG_UP:   Show previous image\n"
" HOME:                 Show first image\n"
" END:                  Show last image\n"
" LEFT_ARROW:           Scroll image left\n"
" RIGHT_ARROW:          Scroll image right\n"
" UP_ARROW:             Scroll image up\n"
" DOWN_ARROW:           Scroll image down\n"
" F:                    Fit image\n"
" + or ]:               Zoom in\n"
" - or [:               Zoom out\n"
" 1:                    Restore original image size\n"
" K:                    Remember zoom and rotation settings\n"
" F5:                   Refresh image\n"
" R:                    Rotate image 90 degrees clockwise\n"
" L:                    Rotate image 90 degrees counterclockwise\n"
" S:                    Start/Stop slideshow\n"
" F11:                  Toggle fullscreen mode\n"
" ESC:                  Close viewer frame\n"
msgstr ""
" SPACJA lub PG_DOWN:	Pokaż nastepny obraz\n"
" BACKSPACE lub PG_UP:	Pokaż poprzedni obraz\n"
" HOME:			Pokaż pierwszy obraz\n"
" END:			Pokaż ostatni obraz\n"
" STRZAŁKA W LEWO:	Przesuń obraz w lewo\n"
" STRZAŁKA W PRAWO:	Przesuń obraz w prawo\n"
" STRZAŁKA W GÓRĘ:	Przesuń obraz w górę\n"
" STRZAŁKA W DÓŁ:	Przesuń obraz w dół\n"
" F:			Dopasuj obraz\n"
" + lub ]:		Powiększ\n"
" - lub [:		Pomniejsz\n"
" 1:			Przywróć oryginalny rozmiar obrazu\n"
" K:			Zapamiętaj powiększenie i rotację obrazu\n"
" F5:			Odśwież obraz\n"
" R:			Obróć obraz o 90 stopni w prawo\n"
" L:			Obróć obraz o 90 stopni w lewo\n"
" S:			Start/Stop pokazu slajdów\n"
" F11:			Tryb pełnoekranowy\n"
" ESC:			Zamknij okno obrazu\n"

